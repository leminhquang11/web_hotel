                    <select name="" id="" class="btn btn-default btn-sm">
                        <option value="1">1</option>
                        <option value="1">2</option>
                        <option value="1">3</option>
                    </select>
                </td>
                <td class="slht-td-total" rowspan="400">
                    <p>1 room for</p>
                    <p class="slht-btn-price">VND <span>1,133,000</span></p>
