<div class="col-xs-12 no-padding result-slht-table">
    <table class="table table-bordered table-responsive slht-table">
        <thead>
            <th><%= t "Guests" %></th>
            <th><%= t "Room_Types" %></th>
            <th><%= t "Price_per_night" %></th>
            <th class="slht-th-choices"><%= t "Facilities" %></th>
            <th><%= t "Select_Room" %></th>
            <th></th>
        </thead>
        <tbody>
            <tr>
                <td class="slht-td-sleep">
